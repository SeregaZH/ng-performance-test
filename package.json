{
  "name": "angular2-tour-of-heroes",
  "version": "0.1.0",
  "scripts": {
    "start": "gulp copy-jit && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "aot": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "start-aot": "npm run aot && npm run rollup && gulp copy-aot && npm run lite"
  },
  "dependencies": {
    "@angular-redux/store": "6.2.0",
    "@angular/animations": "4.0.2",
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/compiler-cli": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/forms": "4.0.2",
    "@angular/http": "4.0.2",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
    "@angular/platform-server": "4.0.2",
    "@angular/router": "4.0.2",
    "core-js": "2.4.1",
    "lodash": "4.17.4",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "rxjs": "5.3.0",
    "web-animations-js": "2.2.2",
    "zone.js": "0.8.5",
    "systemjs": "0.19.40"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "2.53.33",
    "codelyzer": "^2.0.0-beta.4",
    "concurrently": "^3.1.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-load-plugins": "^1.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.7",
    "lite-server": "^2.2.2",
    "rollup": "^0.36.1",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "tslint": "^4.1.1",
    "typescript": "^2.0.10"
  }
